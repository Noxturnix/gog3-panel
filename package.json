{
  "name": "gog3-panel",
  "version": "1.0.0",
  "main": "electron/main.ts",
  "scripts": {
    "dev:svelte": "svelte-kit dev --port 3001",
    "dev:electron": "electron .",
    "build:svelte": "svelte-kit build",
    "build:electron:linux": "electron-builder -l --config build.config.json",
    "build:electron:windows": "electron-builder -w --config build.config.json",
    "build:electron:macos": "electron-builder -m --config build.config.json",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "next",
    "electron": "^18.0.3",
    "electron-builder": "^23.0.3",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "electron-serve": "^1.1.0",
    "ws": "^8.5.0"
  }
}
